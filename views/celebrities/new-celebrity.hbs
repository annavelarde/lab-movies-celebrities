<h2>New Celebrity</h2>
<hr /><br />
<form action="/celebrities/create" method="POST">
<label>  Name:
  <input type="text" name="name" placeholder="The Celeb Name"  value="{{this.name}}"/>
  </label>
  <br /><br />
<label>
    Occupation:
    <input type="text" name="occupation" placeholder="The Celeb Occupation" value="{{this.occupation}}"/>
</label>
  <br /><br />
<label>
    Catch Phrase:
    <input type="text" name="catchPhrase" placeholder="The Celeb Catch Phrase" value="{{this.catchPhrase}}"/>
</label>
  <button type:"submit">Submit</button>
</form>
